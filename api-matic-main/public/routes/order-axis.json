{
  "name": "order-axis",
  "title": "Paket Data Axis",
  "description": "Melakukan pemesanan paket data Axis ke nomor tujuan.",
  "method": "POST",
  "path": "/api/order-axis",
  "has_product_dropdown": true,
  "product_options": {
    "get_products_endpoint": "/api/get-axis-products",
    "group_by": "provider.name",
    "sort_by": "price"
  },
  "parameters": [
    {
      "type": "text",
      "name": "auth_token",
      "label": "Auth Token",
      "placeholder": "Token dari API OrderKuota"
    },
    {
      "type": "text",
      "name": "auth_username",
      "label": "Auth Username",
      "placeholder": "Username dari API OrderKuota"
    },
    {
      "type": "text",
      "name": "target_number",
      "label": "Nomor Axis Tujuan",
      "placeholder": "083812345678"
    },
    {
      "type": "select",
      "name": "voucher_id",
      "label": "Pilih Paket Data Axis"
    }
  ],
  "response": {
    "success": true,
    "status": "Sukses",
    "message": "Order sedang diproses.",
    "transaction_details": {
      "transaction_id": 22334455,
      "target_number": "083812345678",
      "product_name": "AXIS DATA 5GB",
      "price": "Rp 25.000",
      "date": "03/07/2025 02:30"
    }
  },
  "curl": "curl -X POST https://bovalone.me/api/order-axis \\\n  -H \"Content-Type: application/json\" \\\n  -d '{\"auth_token\":\"...\", \"auth_username\":\"...\", \"target_number\":\"083812345678\", \"voucher_id\":\"...\"}'",
  "category": "Order Kuota"
}