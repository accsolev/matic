{
  "name": "order-cekh2h",
  "title": "Cek Akun Digital (H2H)",
  "description": "Melakukan pengecekan nama pemilik akun/game (AOV, Dana, Free Fire, dll).",
  "method": "POST",
  "path": "/api/order-cekh2h",
  "has_product_dropdown": true,
  "product_options": {
    "get_products_endpoint": "/api/get-cekh2h-products",
    "group_by_static": "Layanan Pengecekan",
    "sort_by": "name"
  },
  "parameters": [
    { "type": "text", "name": "auth_token", "label": "Auth Token", "placeholder": "Token dari API OrderKuota" },
    { "type": "text", "name": "auth_username", "label": "Auth Username", "placeholder": "Username dari API OrderKuota" },
    { "type": "text", "name": "target_number", "label": "Nomor / ID Akun", "placeholder": "ID Akun Game atau Nomor HP" },
    { "type": "select", "name": "voucher_id", "label": "Pilih Layanan Cek Akun" }
  ],
  "response": { "success": true, "status": "Sukses", "message": "Order sedang diproses." },
  "curl": "curl -X POST https://bovalone.me/api/order-cekh2h \\\n  -H \"Authorization: Bearer {API_KEY}\" \\\n  -H \"Content-Type: application/json\" \\\n  -d '{\"auth_token\":\"...\", \"auth_username\":\"...\", \"target_number\":\"...\", \"voucher_id\":\"5233\"}'",
  "category": "OrderKuota [ Digital Order ]"
}
