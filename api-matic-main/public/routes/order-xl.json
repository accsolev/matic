{
  "name": "order-xl",
  "title": "Paket Data XL",
  "description": "Melakukan pemesanan paket data XL ke nomor tujuan.",
  "method": "POST",
  "path": "/api/order-xl",
  "has_product_dropdown": true,
  "product_options": {
    "get_products_endpoint": "/api/get-xl-products",
    "group_by": "provider.name",
    "sort_by": "price"
  },
  "parameters": [
    {
      "type": "text",
      "name": "auth_token",
      "label": "Auth Token",
      "placeholder": "Token dari API OrderKuota"
    },
    {
      "type": "text",
      "name": "auth_username",
      "label": "Auth Username",
      "placeholder": "Username dari API OrderKuota"
    },
    {
      "type": "text",
      "name": "target_number",
      "label": "Nomor XL Tujuan",
      "placeholder": "087812345678"
    },
    {
      "type": "select",
      "name": "voucher_id",
      "label": "Pilih Paket Data XL"
    }
  ],
  "response": {
    "success": true,
    "status": "Sukses",
    "message": "Order sedang diproses.",
    "transaction_details": {
      "transaction_id": 66778899,
      "target_number": "087812345678",
      "product_name": "XL DATA 15GB",
      "price": "Rp 75.000",
      "date": "03/07/2025 04:30"
    }
  },
  "curl": "curl -X POST https://bovalone.me/api/order-xl \\\n  -H \"Content-Type: application/json\" \\\n  -d '{\"auth_token\":\"...\", \"auth_username\":\"...\", \"target_number\":\"087812345678\", \"voucher_id\":\"...\"}'",
  "category": "Order Kuota"
}